{
  "compilerOptions": {
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "lib": ["esnext", "dom"],
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": ".",
    "noImplicitReturns": false,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": ["src/**/*"]
}
